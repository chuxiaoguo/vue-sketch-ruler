{
    "name": "vue-sketch-ruler",
    "version": "1.0.3",
    "description": "a sketch like ruler in javasript or vue",
    "main": "dist/vue-sketch-ruler.umd.js",
    "module": "dist/vue-sketch-ruler.esm.js",
    "unpkg": "dist/vue-sketch-ruler.min.js",
    "scripts": {
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@rollup/plugin-alias": "^2.2.0",
        "@rollup/plugin-buble": "^0.20.0",
        "@rollup/plugin-replace": "^2.2.1",
        "babel-loader": "^8.0.6",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-3": "^6.24.1",
        "cross-env": "^6.0.3",
        "minimist": "^1.2.0",
        "rollup": "^1.26.3",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-terser": "^5.1.2",
        "rollup-plugin-vue": "6.0.0",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10"
    },
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "keywords": [
        "vue",
        "ruler",
        "javascript",
        "sketch-ruler",
        "vue-ruler",
        "vue-sketch-ruler"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/chuxiaoguo/vue-sketch-ruler"
    },
    "author": "chuxiaoguo <408755140@qq.com>",
    "license": "MIT",
    "files": [
        "dist/*",
        "src/**/*.vue",
        "!src/lib-dev.vue"
    ]
}
